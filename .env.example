# Publer API Configuration
# Copy this file to .env and fill in your actual values

# Your Publer API key (required)
# Get it from: https://publer.com/help/en/article/how-to-access-the-publer-api-1w08edo/
PUBLER_API_KEY=your_api_key_here

# Optional: Default workspace ID
# If set, this workspace will be used by default for all requests
PUBLER_WORKSPACE_ID=

# Optional: API base URL (only change if using a different environment)
PUBLER_BASE_URL=https://app.publer.com/api/v1

# Optional: Request timeout in seconds (default: 30)
PUBLER_TIMEOUT=30

# Optional: Enable debug logging (true/false)
PUBLER_DEBUG=false
